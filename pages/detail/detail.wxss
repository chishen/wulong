pagg{
    height: 100%;
}
.table{
    height: 100%;
    margin-top: 24rpx;
}
.table .tr{
    width: 600rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    margin-left: auto;
    margin-right: auto;
    border-bottom: 1rpx solid #ddd;
    padding: 8rpx;
}
.table .tr .th{
    font-size: 24rpx;
    color: #999;
    padding-right: 24rpx;
}
.table .tr .td{
    color: #333;
    font-size: 24rpx;
}